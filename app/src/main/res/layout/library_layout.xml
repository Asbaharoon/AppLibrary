<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:id="@+id/lib_scrollView"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:scrollbars="none"
    android:fillViewport="true"
    android:fitsSystemWindows="true"

    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        >
        <FrameLayout
            android:id="@+id/libraryPhoto_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/_5sdp"
            android:background="@drawable/add_photo_tv"
            android:clickable="true"
            android:descendantFocusability="blocksDescendants"
            >
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/library_photo"
                android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                app:civ_border_width="@dimen/_1sdp"
                app:civ_border_color="@color/base"
                android:layout_gravity="center_vertical"
                android:clickable="false"
                android:longClickable="false"

                />
            <TextView
                android:id="@+id/addPhoto_tv_lib"
                android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                android:textColor="@color/base"
                android:textSize="@dimen/_12sdp"
                android:text="@string/add_photo"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:background="@drawable/add_photo_tv"
                android:padding="@dimen/_3sdp"
                android:clickable="false"
                android:longClickable="false"
                />
        </FrameLayout>
        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/library_Name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:met_floatingLabel="highlight"
            app:met_helperText="@string/required"
            app:met_autoValidate="true"
            app:met_floatingLabelAnimating="true"
            app:met_singleLineEllipsis="true"
            app:met_helperTextAlwaysShown="true"
            app:met_floatingLabelAlwaysShown="true"
            app:met_iconLeft="@drawable/library_icon"
            app:met_floatingLabelText="@string/lib_name"
            app:met_floatingLabelTextColor="@color/base"
            app:met_helperTextColor="@color/label"
            app:met_textColor="@color/base"
            app:met_underlineColor="@color/label"
            app:met_errorColor="@color/error"
            app:met_baseColor="@color/base"
            app:met_primaryColor="@color/base"
            app:met_textColorHint="@color/base"
            android:textSize="@dimen/_16sdp"
            android:maxLength="50"
            android:inputType="textAutoComplete"
            android:gravity="left"



            />
        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/library_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:met_floatingLabelAnimating="true"
            app:met_floatingLabel="highlight"
            app:met_floatingLabelText="@string/lib_type"
            app:met_floatingLabelAlwaysShown="true"
            app:met_helperTextAlwaysShown="true"
            app:met_checkCharactersCountAtBeginning="true"
            app:met_singleLineEllipsis="true"
            app:met_iconLeft="@drawable/library_icon"
            app:met_floatingLabelTextColor="@color/base"
            app:met_helperTextColor="@color/label"
            app:met_textColor="@color/base"
            app:met_underlineColor="@color/label"
            app:met_errorColor="@color/error"
            app:met_baseColor="@color/base"
            app:met_primaryColor="@color/base"
            app:met_textColorHint="@color/base"
            android:textSize="@dimen/_16sdp"
            app:met_helperText="@string/required"
            android:focusable="false"
            android:clickable="true"
            android:gravity="left"



            />

        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/library_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            app:met_autoValidate="true"
            app:met_floatingLabelAnimating="true"
            app:met_floatingLabel="highlight"
            app:met_floatingLabelText="@string/e_mail_address"
            app:met_floatingLabelAlwaysShown="true"
            app:met_helperText="@string/required"
            app:met_helperTextAlwaysShown="true"
            app:met_checkCharactersCountAtBeginning="true"
            app:met_singleLineEllipsis="true"
            app:met_iconLeft="@drawable/email_icon"
            app:met_floatingLabelTextColor="@color/base"
            app:met_helperTextColor="@color/label"
            app:met_textColor="@color/base"
            app:met_underlineColor="@color/label"
            app:met_errorColor="@color/error"
            app:met_baseColor="@color/base"
            app:met_primaryColor="@color/base"
            app:met_textColorHint="@color/base"
            android:textSize="@dimen/_16sdp"
            android:maxLength="30"




            />

        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/library_phone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:met_autoValidate="true"
            app:met_floatingLabelAnimating="true"
            app:met_floatingLabel="highlight"
            app:met_floatingLabelText="@string/phone"
            app:met_floatingLabelAlwaysShown="true"
            app:met_helperText="@string/required"
            app:met_helperTextAlwaysShown="true"
            app:met_checkCharactersCountAtBeginning="true"
            app:met_singleLineEllipsis="true"
            app:met_iconLeft="@drawable/phone_icon"
            app:met_floatingLabelTextColor="@color/base"
            app:met_helperTextColor="@color/label"
            app:met_textColor="@color/base"
            app:met_underlineColor="@color/label"
            app:met_errorColor="@color/error"
            app:met_baseColor="@color/base"
            app:met_primaryColor="@color/base"
            app:met_textColorHint="@color/base"
            android:textSize="@dimen/_16sdp"
            android:focusable="false"
            android:clickable="true"
            android:gravity="left"



            />

        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/library_country"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:met_autoValidate="true"
            app:met_floatingLabelAnimating="true"
            app:met_floatingLabel="highlight"
            app:met_floatingLabelText="@string/country"
            app:met_floatingLabelAlwaysShown="true"
            app:met_helperText="@string/required"
            app:met_helperTextAlwaysShown="true"
            app:met_checkCharactersCountAtBeginning="true"
            app:met_singleLineEllipsis="true"
            app:met_iconLeft="@drawable/location_icon"
            app:met_floatingLabelTextColor="@color/base"
            app:met_helperTextColor="@color/label"
            app:met_textColor="@color/base"
            app:met_underlineColor="@color/label"
            app:met_errorColor="@color/error"
            app:met_baseColor="@color/base"
            app:met_primaryColor="@color/base"
            app:met_textColorHint="@color/base"
            android:textSize="@dimen/_16sdp"
            android:focusable="false"
            android:clickable="true"
            android:gravity="left"







            />

        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/library_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:met_autoValidate="true"
            app:met_floatingLabelAnimating="true"
            app:met_floatingLabel="highlight"
            app:met_floatingLabelText="@string/reg_address"
            app:met_floatingLabelAlwaysShown="true"
            app:met_helperText="@string/required"
            app:met_helperTextAlwaysShown="true"
            app:met_checkCharactersCountAtBeginning="true"
            app:met_singleLineEllipsis="true"
            app:met_iconLeft="@drawable/location_icon"
            app:met_floatingLabelTextColor="@color/base"
            app:met_helperTextColor="@color/label"
            app:met_textColor="@color/base"
            app:met_underlineColor="@color/label"
            app:met_errorColor="@color/error"
            app:met_baseColor="@color/base"
            app:met_primaryColor="@color/base"
            app:met_textColorHint="@color/base"
            android:textSize="@dimen/_16sdp"
            android:inputType="textAutoComplete"
            android:maxLength="100"






            />

        <!--<RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
